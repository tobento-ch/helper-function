{
    "name": "tobento/helper-function",
    "type": "library",
    "description": "Support for helper functions management.",
    "keywords": [
        "tobento",
        "package",
        "helper",
        "function"
    ],
    "homepage": "https://www.tobento.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Strub",
            "email": "info@tobento.ch",
            "homepage": "https://www.tobento.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,    
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },    
    "autoload": {
        "psr-4": {
            "Tobento\\Service\\HelperFunction\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tobento\\Service\\HelperFunction\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always"
    }
}
